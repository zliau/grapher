{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "comma-dangle": ["error", "never"],
    "eol-last": ["error", "never"],
    "quotes": ["error", "double"],
    "indent": ["error", 4]
  },
  "env": {
    "browser": true
  }
}
